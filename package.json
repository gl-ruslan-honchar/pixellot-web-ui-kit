{
  "name": "pixellot-web-ui-kit",
  "version": "0.0.3",
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "dev-consumer": "vite --config ./vite.config.consumer.ts --port 3300",
    "build-lib": "vite build --config ./vite.config.lib.ts && vue-tsc --emitDeclarationOnly",
    "build-consumer": "vite build --config ./vite.config.consumer.ts"
  },
  "files": ["dist"],
  "type": "module",
  "main": "./dist/lib/pixellot-web-ui-kit.es.js",
  "module": "./dist/lib/pixellot-web-ui-kit.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": "./dist/lib/pixellot-web-ui-kit.es.js",
    "./style.css": "./dist/lib/style.css"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "sass": "^1.52.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
