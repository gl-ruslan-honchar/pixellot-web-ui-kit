{
  "name": "pixellot-web-ui-kit",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "files": ["dist"],
  "main": "./dist/pixellot-web-ui-kit.umd.js",
  "module": "./dist/pixellot-web-ui-kit.es.js",
  "types": "./dist/types/main-lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/pixellot-web-ui-kit.es.js",
      "require": "./dist/pixellot-web-ui-kit.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "sass": "^1.52.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  }
}
